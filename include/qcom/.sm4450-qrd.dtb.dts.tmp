  };

  camcc: clock-controller@ade0000 {
   compatible = "qcom,sm4450-camcc";
   reg = <0x0 0x0ade0000 0x0 0x20000>;
   clocks = <&rpmhcc 0>,
     <&gcc 5>;
   #clock-cells = <1>;
   #reset-cells = <1>;
   #power-domain-cells = <1>;
  };

  dispcc: clock-controller@af00000 {
   compatible = "qcom,sm4450-dispcc";
   reg = <0x0 0x0af00000 0x0 0x20000>;
   clocks = <&rpmhcc 0>,
     <&rpmhcc 1>,
     <&gcc 14>,
     <&sleep_clk>,

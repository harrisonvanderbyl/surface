# 0 "arch/arm64/boot/dts/qcom/sc8280xp-el2.dtso"
# 0 "<built-in>"
# 0 "<command-line>"
# 1 "arch/arm64/boot/dts/qcom/sc8280xp-el2.dtso"






/dts-v1/;
/plugin/;


&gpu {
 zap-shader {
  status = "disabled";
 };
};






&pcie2a {
 iommu-map = <0 &pcie_smmu 0x20000 0x10000>;
};

&pcie2b {
 iommu-map = <0 &pcie_smmu 0x30000 0x10000>;
};

&pcie3a {
 iommu-map = <0 &pcie_smmu 0x40000 0x10000>;
};

&pcie3b {
 iommu-map = <0 &pcie_smmu 0x50000 0x10000>;
};

&pcie4 {
 iommu-map = <0 &pcie_smmu 0x60000 0x10000>;
};

&pcie_smmu {
 status = "okay";
};

&remoteproc_adsp {
 qcom,broken-reset;
};

&remoteproc_slpi {
 qcom,broken-reset;
};

&remoteproc_nsp0 {
 qcom,broken-reset;
};

&remoteproc_nsp1 {
 qcom,broken-reset;
};

&venus {
 video-firmware {
  iommus = <&apps_smmu 0x2a02 0x400>;
 };
};
